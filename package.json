{
  "name": "recipe-to-list",
  "version": "1.0.0",
  "description": "Starting point for react/node, includes webpack, hot loader, and airbnb eslint.",
  "repository": {
    "type": "git",
    "url": "git://github.com/scwood/recipe-to-list.git"
  },
  "scripts": {
    "build": "webpack -p && babel src/server -d dist",
    "lint": "eslint src",
    "start": "npm run start-client & npm run start-server",
    "start-client": "webpack-dev-server --no-info --progress",
    "start-server": "nodemon src/server/index.js --exec babel-node --ignore src/client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Spencer Wood",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "html-webpack-plugin": "^2.15.0",
    "nodemon": "^1.9.1",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
